{
    "name": "Stretching Alarm",
    "version": "1.0",
    "description": "Alarm for stretching 50min interval!",
    "permissions": [
        "storage",
        "activeTab",
        "notifications"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "manifest_version": 2
}